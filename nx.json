{
  "npmScope": "flutter-weather",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "mobile_flutter": {
      "tags": []
    },
    "firebase-functions": {
      "tags": []
    },
    "firebase-hosting": {
      "tags": []
    },
    "firebase-rules": {
      "tags": []
    },
    "web_ng": {
      "tags": []
    },
    "web_ng_e2e": {
      "tags": [],
      "implicitDependencies": ["web_ng"]
    }
  }
}
